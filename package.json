{
  "name": "vue-nav-wheel",
  "version": "0.1.0",
  "license": "MIT",
  "description": "",
  "author": {
    "name": "Richard Webb",
    "email": "rwebb2305@gmail.com",
    "url": "https://www.linkedin.com/in/richard-webb-09516a115/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jaeger2305/vue-nav-wheel"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name nav-wheel ./src/entry.js",
    "build:demo": "vue-cli-service build",
    "test": "npm run test:unit",
    "test:unit": "vue-cli-service test:unit",
    "test:debug": "node --inspect-brk node_modules/.bin/vue-cli-service test:unit --no-cache --watch --runInBand",
    "lint": "vue-cli-service lint"
  },
  "main": "dist/nav-wheel.umd.min.js",
  "module": "dist/nav-wheel.esm.js",
  "browser": {
    "./sfc": "src/components/nav-wheel.vue"
  },
  "unpkg": "dist/nav-wheel.min.js",
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-plugin-unit-jest": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-eslint": "^10.1.0",
    "d3-shape": "^1.3.7",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.1",
    "husky": "^4.2.3",
    "lodash": "^4.17.15",
    "sass": "^1.26.2",
    "sass-loader": "^8.0.2",
    "sinon": "^9.0.0",
    "uuid": "^3.4.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.11"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "ignoreRestSiblings": true
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/*.{spec,test}.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "keywords": [
    "vue",
    "router",
    "navbar",
    "navigation"
  ]
}
